<Application
    StartupUri="UI/Forms/WorkingEnvironmentWindow.xaml"
    x:Class="WpfFileManager.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:local="clr-namespace:WpfFileManager"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="UI/Styles/Colors/Colors.xaml" />
                <ResourceDictionary Source="UI/Styles/Styles/MainStyle.xaml" />
                <ResourceDictionary Source="UI/Styles/Icons/Icons.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
